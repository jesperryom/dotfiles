{
  "coc.preferences.extensionUpdateCheck": "daily",
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.level": "warning",
  "diagnostic.maxWindowHeight": 100,
  "diagnostic.maxWindowWidth": 150,
  "diagnostic.warningSign": "",
  "dialog.maxHeight": 100,
  "dialog.maxWidth": 150,
  "eslint.autoFixOnSave": true,
  "git.addGBlameToVirtualText": true,
  "git.showCommitInFloating": true,
  "highlight.colorNames.enable": false,
  "html.validate.scripts": true,
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": ["typescript", "typescriptreact", "graphql", "javascript", "javascriptreact"]
    }
  },
  "notification.maxHeight": 100,
  "notification.maxWidth": 150,
  "prettier.proseWrap": "always",
  "signature.maxWindowHeight": 100,
  "signature.maxWindowWidth": 150,
  "snippets.extends": {
    "typescriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "suggest.maxPreviewWidth": 150,
  "tsserver.formatOnType": true
}
